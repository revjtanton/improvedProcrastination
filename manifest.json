{
  "manifest_version": 2,

  "name": "Improved Procrastination",
  "description": "Redirect a time-suck page to something less sucky!",
  "version": "1.0",

  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "storage",
    "cookies"
  ],
  "content_scripts": [ {
    "js": ["script.js"],
    "matches": [ "http://*/*", "https://*/*" ]
  } ]
}
